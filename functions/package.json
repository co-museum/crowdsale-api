{
  "name": "functions",
  "version": "v0.0.0",
  "scripts": {
    "format": "eslint --fix --ext .js,.ts .",
    "lint": "eslint --ext .js,.ts .",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "serve": "yarn build && firebase emulators:start --only functions",
    "shell": "yarn build && firebase functions:shell",
